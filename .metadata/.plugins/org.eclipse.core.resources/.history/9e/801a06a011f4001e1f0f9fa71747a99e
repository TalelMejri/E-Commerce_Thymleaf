package com.mycompany.product.repository;

import com.mycompany.product.model.MyUser;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import java.util.Optional;

@Repository
public interface MyUserRepository extends JpaRepository<MyUser, Long> {
    // Ajoutez ici des méthodes personnalisées de requête si nécessaire

    Optional<MyUser> findByUsernameAndPassword(String username, String password);
}
